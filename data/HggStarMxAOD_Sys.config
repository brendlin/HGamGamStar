# Release 21 base configuraiton
Include:                    HGamGamStar/HggStarMxAOD.config

# No skimming applied
SkimmingCut:                -1

# Whether to save the good objects (xAOD format)
# See MxAODCommon.config for which variables
# are saved for each object
SaveObjects:		        NO

  # whether to save truth objects and variables
SaveTruthObjects:	        NO

SaveDetailedVariables:          NO
SaveTruthVariables:	        NO

  # Save systematic variations
ApplySystematicVariations:      YES

  # Whether to only write specific uncertainty components
HgammaAnalysis.UncertaintyComponents:  *
HgammaAnalysis.IgnoreUncertaintyComponents: *Trig* FT_* MET_* 

MxAOD.Variables.HGamEventInfo: m_lly.m_lly_gev.yyStarCategory.isPassedEventSelection.weight.crossSectionBRfilterEff.cutFlow
MxAOD.Variables.EventInfoSyst: m_lly.m_lly_gev.yyStarCategory.isPassedEventSelection.weight.cutFlow
MxAOD.Variables.EventInfo:     runNumber

# Our sample 345961 is absent from HGamAnalysisFramework/HGamRel21.config
EventHandler.HiggsWeights.NNLOPS: 343981 345316 345961
ggF.mcChannelNumbers: 341000 343981 345306 345944 345961
